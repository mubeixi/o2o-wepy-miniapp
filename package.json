    "@wepy/babel-plugin-import-regenerator": "0.0.2",
    "@wepy/cli": "2.0.0-alpha.25",
    "@wepy/compiler-babel": "2.0.5",
    "@wepy/compiler-less": "2.0.7",
    "@wepy/compiler-sass": "2.0.2",
